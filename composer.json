{
    "name": "tm4b/tm4b-php",
    "type": "library",
    "description": "PHP Client for using TM4B's REST API",
    "keywords": [
        "tm4b",
        "sms",
        "api"
    ],
    "homepage": "https://github.com/tm4b-com/tm4b-php",
    "license": "MIT",
    "authors": [
        {
            "name": "TM4B API Team",
            "email": "api@tm4b.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "php-http/guzzle6-adapter": "^1.0",
        "zendframework/zend-diactoros": "^1.7",
        "webmozart/assert": "^1.2"
    },
    "autoload": {
        "psr-0": {
            "Tm4b": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "vlucas/phpdotenv": "^2.4",
        "php-http/mock-client": "^1.0",
        "helmich/phpunit-psr7-assert": "^1.0"
    }
}
